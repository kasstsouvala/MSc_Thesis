flare = qgis.utils.iface.activeLayer()
#flare.featureCount()
i =round (-62.3600, 4)
j =round (-5.3600, 4)

def coorF(point, raster):
    return flare.dataProvider().identify(QgsPoint(i,j), QgsRaster.IdentifyFormatValue).results().values()
     #print (res.results(), i, j)


while i :
